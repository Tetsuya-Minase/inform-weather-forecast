{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es6"],
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["node", "reflect-metadata"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}